{
  "name": "minapp-comp-definition",
  "publisher": "Yrobot",
  "displayName": "小程序自定义组件标签跳转到文件",
  "description": "可以在wxml页面，'alt + 点击自定义组件标签名'跳转到相对应的组件页面",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:wxml"
  ],
  "main": "./out/extension.js",
  "icon": "logo.png",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "eslint": "^6.8.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "repository": "https://github.com/wjf2016/minapp-comp-definition"
}